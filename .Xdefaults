!Urxvt Part
URxvt.font:         xft:Hack-Regular:pixelsize=18:antialias=true,xft:wenquanyi\ micro\ hei:pixelsize=16:antialias=true,xft:Symbola:pixelsize=13:antialias=true
URxvt.boldFont:     xft:Hack-Bold:pixelsize=18:weight=bold,xft:Symbola
URxvt.italicFont:   xft:Hack-RegularOblique:pixelsize=18:slant=italic,xft:Symbola
!URxvt.letterSpace: 0

urxvt.scrollstyle: plain
URxvt.scrollColor: #000000
URxvt.scrollBar_right: true
!URxvt.mouseWheelScrollPage:false
!URxvt.scrollTtyOutput:false
!URxvt.scrollWithBuffer:false
!URxvt.scrollTtyKeypress:false
URxvt*saveLines: 100000

!URxvt.selection.pattern-0: >>>[ ]+(.+)
!URxvt.selection.pattern-0: /\w/
URxvt.selection.pattern-0: ([0-9a-zA-Z_]+)
URxvt.selection.pattern-1: ([0-9a-zA-Z_\-]+)

URxvt.perl-lib: /usr/lib/urxvt/perl
!URxvt.perl-ext-common: default,url-select,keyboard-select,selection,clipboard,selection-to-clipboard,selection-pastebin
URxvt.perl-ext: selection,clipboard,selection-to-clipboard,selection-pastebin
URxvt.clipboard.autocopy: true

URxvt.iconFile: /home/song/.local/share/icons/hicolor/large/terminal.png

URxvt.borderLess:true

URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt.clipboard.copycmd: xsel -ib
URxvt.clipboard.pastecmd: xsel -ob

URxvt*termName: rxvt-256color
!URxvt*termName: xterm-256color
!Urxvt*termName: screen-256color

Xft*antialias:          true


